<use master="" />
<viewdata model="FubuMVC.Diagnostics.Notifications.NoOutputsNotification" />
<content:main>
  <div class="notification">
    <p>
      <FilterLink column="${Model.Filter.ColumnName}" value="${Model.Filter.Value}" text="${Model.BehaviorCount.ToString()} Missing Output Nodes" />
    </p>
    <p class="body">
      You may want to check your views. Have you installed a view engine?
    </p>
  </div>
</content:main>